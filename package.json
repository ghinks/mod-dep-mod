{
  "name": "mod-dep-mod",
  "version": "4.0.1",
  "type": "module",
  "description": "tool to find modules that depend on other modules, comes in handy when you don't know what is pulling in what",
  "bin": {
    "mod-dep-mod": "bin/index.js"
  },
  "scripts": {
    "lint": "standard --env mocha src/**/*.js",
    "test": "cross-env NODE_ENV=test npx mocha --loader=testdouble --timeout 4000 src/**/*.spec.js"
  },
  "engines": {
    "node": ">=16.13.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ghinks/mod-dep-mod.git"
  },
  "keywords": [
    "search",
    "find",
    "module",
    "dependencies",
    "dependency",
    "tree",
    "dep",
    "compliance",
    "risk",
    "security"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "async": "3.2.3",
    "isomorphic-fetch": "3.0.0",
    "minimist": "1.2.6",
    "npm-package-arg": "8.1.5",
    "ora": "6.0.1",
    "pify": "5.0.0",
    "read-package-json": "4.1.1",
    "registry-url": "5.1.0",
    "semver": "7.3.5"
  },
  "devDependencies": {
    "chai": "4.4.1",
    "cross-env": "7.0.3",
    "mocha": "9.2.0",
    "nock": "13.2.4",
    "nodemon": "2.0.15",
    "standard": "16.0.4",
    "testdouble": "3.16.4"
  },
  "standard": {
    "ignore": "lib",
    "env": {
      "mocha": true
    }
  }
}

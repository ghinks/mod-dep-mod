{
  "name": "mod-dep-mod",
  "version": "2.0.4",
  "description": "",
  "bin": "bin/index.js",
  "scripts": {
    "build": "npx babel src --out-dir lib --ignore \"src/**/test.js\"",
    "watch": "npx babel --watch src --out-dir lib --ignore \"src/**/test.js\"",
    "lint": "npx standard --env mocha src/**/*.js",
    "test": "NODE_ENV=test npx mocha --timeout 4000 --require @babel/register src/**/*test.js",
    "cover": "NODE_ENV=test npx nyc mocha --timeout 4000 ./src/**/test.js --require @babel/register",
    "cover:html": "NODE_ENV=test npx nyc --reporter=html npm run cover "
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ghinks/mod-dep-mod.git"
  },
  "keywords": [
    "search",
    "find",
    "module",
    "dependencies",
    "dependency",
    "tree",
    "dep",
    "compliance",
    "risk",
    "security"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "7.1.5",
    "async": "3.0.1",
    "isomorphic-fetch": "2.2.1",
    "minimist": "1.2.0",
    "npm-package-arg": "6.1.0",
    "ora": "3.4.0",
    "pify": "4.0.1",
    "read-package-json": "2.0.13",
    "registry-url": "5.1.0",
    "semver": "6.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.1.5",
    "@babel/core": "7.1.6",
    "@babel/plugin-transform-runtime": "7.1.0",
    "@babel/polyfill": "7.0.0",
    "@babel/preset-env": "7.1.6",
    "@babel/register": "7.0.0",
    "babel-eslint": "10.0.1",
    "babel-plugin-rewire": "1.2.0",
    "chai": "4.2.0",
    "mocha": "6.1.4",
    "nock": "10.0.6",
    "nodemon": "1.18.7",
    "nyc": "14.0.0",
    "standard": "12.0.1"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": "lib",
    "env": {
      "mocha": true
    }
  },
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/**/test.js"
    ],
    "report": [
      "json",
      "lcov",
      "html",
      "text"
    ]
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "6.0.0"
          }
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ],
    "env": {
      "test": {
        "plugins": [
          "babel-plugin-rewire",
          "@babel/plugin-transform-runtime"
        ]
      }
    }
  }
}

{
  "name": "mod-dep-mod",
  "version": "0.0.3",
  "description": "",
  "bin": "bin/index.js",
  "scripts": {
    "build": "babel src --out-dir lib --ignore test.js",
    "watch": "babel --watch src --out-dir lib --ignore test.js",
    "lint": "standard --env mocha src/**/*.js",
    "test": "NODE_ENV=test mocha --require babel-core/register src/**/test.js",
    "cover": "nyc npm test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ghinks/mod-dep-mod.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {

  },
  "devDependencies": {
    "babel-cli": "^6.26.0"
  },
  "standard": {
    "parser": "babel-eslint",
    "env": {
      "mocha": true
    }
  },
  "nyc": {
    "require": [
      "babel-core/register"
    ],
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/**/test.js"
    ],
    "report": [
      "lcov",
      "text"
    ]
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "4.0.0"
          }
        }
      ],
      "stage-0"
    ],
    "plugins": [
      "transform-runtime"
    ],
    "env": {
      "test": {
        "plugins": [
          "babel-plugin-rewire"
        ]
      }
    }
  }
}

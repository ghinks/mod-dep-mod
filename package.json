{
  "name": "mod-dep-mod",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir lib --ignore test.js",
    "watch": "babel --watch src --out-dir lib --ignore test.js",
    "lint": "standard --env mocha src/**/*.js",
    "test": "mocha --require babel-core/register src/**/test.js",
    "cover": "nyc npm test"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
  },
  "devDependencies": {
    "babel-preset-stage-0": "^6.24.1"
  },
  "standard": {
    "parser": "babel-eslint",
    "env": {
      "mocha": true
    }
  },
  "nyc": {
    "require": [
      "babel-core/register"
    ],
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/**/test.js"
    ],
    "report": [
      "lcov",
      "text"
    ]
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "9"
          }
        }
      ],
      "stage-0"
    ],
    "plugins": [
      "transform-runtime",
      "babel-plugin-rewire"
    ]
  }
}
